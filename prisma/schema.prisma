generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String    @id @default(cuid())
  name     String
  email    String    @unique
  password String
  bookings  Booking[]
}

model Booking {
  id     String   @id @default(uuid())
  date   DateTime
  userId String
  user   User     @relation(fields: [userId], references: [id])
}
